<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/checked"
        android:drawable="@drawable/ic_visibility_eye"
        android:state_checked="true" />
    <item
        android:id="@+id/unchecked"
        android:drawable="@drawable/ic_visibility_eye_strike" />

    <transition
        android:drawable="@drawable/visibility_eye_strike_transition_enter"
        android:fromId="@+id/checked"
        android:toId="@+id/unchecked" />
    <transition
        android:drawable="@drawable/visibility_eye_strike_transition_exit"
        android:fromId="@+id/unchecked"
        android:toId="@+id/checked" />

</animated-selector>